
    <% for(var i=0;i<users.length;i++) { %>
    <tr>
        <td><%= i+1 %></td>
        <td><%= users[i].name %></td>
        <td><%= users[i].phone %></td>
        <td><%= users[i].address %></td>
        <td><%= users[i].debit %></td>
        <td><%= users[i].credit %></td>
        <td><%= users[i].advance %></td>
        <td><%= users[i].loan %></td>
        <td><%= users[i].liability %></td>
        <td><%= users[i].remark %></td>
        <td class="mytd">
            <a href="/update-user?id=<%= users[i]._id %>" class="btn border-shadow update">
                <span class="text-gradient">
                    <i class='fas fa-pencil-alt'></i>
                </span>
            </a>
            <a href="#" class="btn border-shadow delete deleteData" data-id=<%=users[i]._id %>>
                <span class="text-gradient">
                    <i class='fas fa-times'></i>
                </span>
            </a>
        </td>   
    </tr>
    <% } %>